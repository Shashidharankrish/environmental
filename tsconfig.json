{
  "compileOnSave": false,
  "compilerOptions": {
  "strictNullChecks": false,
        "strictFunctionTypes": false,
    "baseUrl": "src",
    "importHelpers": true,
    "paths": {
      "@shared/*": [
        "app/shared/*"
      ]
    },
	"skipLibCheck": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}